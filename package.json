    "@iplab/ngx-color-picker": "2.0.3",
    "axios": "0.21.0",
    "electron-log": "4.2.4",
    "electron-settings": "4.0.2",
    "electron-updater": "4.3.5",
    "fkill": "7.0.1",
    "flag-icon-css": "3.5.0",
    "fs-extra": "9.0.1",
    "glob": "7.1.6",
    "iconv-lite": "0.6.2",
    "lodash": "4.17.20",
    "mapbox-gl": "2.0.0",
    "moment": "2.29.1",
